package com.demo.beans;

public class DoublyCicularLinkedList {
	Node head;
	class Node{
		int data;
		Node next;
	    Node prev;
	    
	    public Node(int n) {
	    	this.data=n;
	    	next=null;
	    	prev=null;
	    }
	}
	DoublyCicularLinkedList(){
		head=null;
	}
	public boolean addAtStart(int n) {
		Node newnode=new Node(n);
		if(head==null) {
			head=newnode;
			newnode.next=head;
			return true;
		}
		else {
			newnode.next=head;
			head.prev=newnode;
			head=newnode;
			return true;
		}
	}
}


