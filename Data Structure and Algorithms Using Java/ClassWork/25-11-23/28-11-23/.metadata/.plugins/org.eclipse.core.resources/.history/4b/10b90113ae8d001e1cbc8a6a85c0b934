package com.demo.beans;

public class SingleLinkedList {
	private Node head;
    class Node{
    	int n;
    	Node next;
    	Node(int d){
    		n=d;
    		next=null;
    	}
    	
    }
    public SingleLinkedList() {
    	head=null;
    }
    
    public void displaydata() {
    	Node temp=head;
    	while(temp!=null) {
    		System.out.println(temp.n);
    		temp=temp.next;
    	}
    }

	public void addatfirst(int i) {
		Node newnode = new Node(i);
		if(head==null) {
			head=newnode;
		}
		else {
			newnode.next=head.next;
			head=newnode;
		}
		
	}
}
