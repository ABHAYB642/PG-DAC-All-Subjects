#include <iostream>
using namespace std;

class demo
{
   
  int a,b;
  
    public: 
  
  // declaring Constructor
  demo(int aa, int bb)
  {
      cout<<"Constructor is called"<<endl;
      a = aa;
      b = bb;
      
      cout<<"Value of a: "<<a<<endl;
      cout<<"Value of b: "<<b<<endl;
      cout<<endl;
  }
  
  // declaring destructor
  ~demo()
  {
      cout<<"Destructor is called"<<endl;
      cout<<"Value of a: "<<a<<endl;
      cout<<"Value of b: "<<b<<endl;
  }
  
};
int main() 
{
    demo obj(5,6);
    
    return 0;
}
/*
Constructor is called
Value of a: 5
Value of b: 6

Destructor is called
Value of a: 5
Value of b: 6
*/